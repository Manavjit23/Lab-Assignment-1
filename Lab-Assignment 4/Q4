#include <iostream>
#include <queue>
using namespace std;

void Q4(string str) {
    queue<char> q;
    int freq[26] = {0};  

    for (char ch : str) {
        freq[ch - 'a']++;
        q.push(ch);        

        while (!q.empty() && freq[q.front() - 'a'] > 1) {
            q.pop();
        }

        if (!q.empty()) {
            cout << q.front() << " ";
        } else {
            cout << "-1 ";
        }
    }

    cout << "\n";
}

int main() {
    string input;
    cout << "Enter a string: ";
    cin >> input;

    Q4(input);

    return 0;
}
