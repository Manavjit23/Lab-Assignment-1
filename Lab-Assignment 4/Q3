#include <iostream>
#include <queue>
using namespace std;

void Q3(queue<int> a) {
    int n = a.size();
    int halfSize = n / 2;

    queue<int> firstHALF;
    queue<int> secondHALF;

    
    for (int i = 0; i < halfSize; i++) {
        firstHALF.push(a.front());
        a.pop();
    }
    for (int i = 0; i < n - halfSize; i++) {
        secondHALF.push(a.front());
        a.pop();
    }


    while (!firstHALF.empty() || !secondHALF.empty()) {
        if (!firstHALF.empty()) {
            cout << firstHALF.front() << " ";
            firstHALF.pop();
        }
        if (!secondHALF.empty()) {
            cout << secondHALF.front() << " ";
            secondHALF.pop();
        }
    }
    cout << "\n";
}

int main() {
    queue<int> a;
    a.push(4);
    a.push(7);
    a.push(11);
    a.push(20);
    a.push(5);
    a.push(6);

    Q3(a); 


    cout << "\n";
}
